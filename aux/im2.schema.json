{
  "$schema": "http://json-schema.org/schema#",
  "type": "array",
  "items": {
    "oneOf": [
      {
        "type": "object",
        "properties": {
          "type": {"type": "string", "pattern": "^cmd$"},
          "cmd": {"type": "string"},
          "params": {
            "type": "array",
            "items": {
              "type": ["string", "integer"]
            }
          }
        }
      },
      {
        "type": "object",
        "properties": {
          "type": {"type": "string", "pattern": "^label$"},
          "number": {"type": "integer"}
        }
      }
    ]
  }
}
